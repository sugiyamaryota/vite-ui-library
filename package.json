{
  "name": "@sugiyamaryota/vite-ui-library",
  "version": "0.0.10",
  "author": "sugiyama ryota <guqeeky6va8@gmail.com>",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/sugiyamaryota/vite-ui-library.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/sugiyamaryota"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "serve": "vite preview",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest",
    "prebuild:storybook": "npm run test:generate-output",
    "build-storybook": "build-storybook -c .storybook -o build/",
    "storybook": "start-storybook -p 6006",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-jest": "^6.3.8",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@storybook/testing-react": "^0.0.22",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.1",
    "prettier": "^2.5.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "storybook-builder-vite": "^0.1.11",
    "tailwindcss": "^3.0.0-alpha.2",
    "ts-jest": "^27.1.2",
    "tsc-alias": "^1.4.1",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts ./src --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  }
}
